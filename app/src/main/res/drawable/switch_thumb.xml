<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Activado -->
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <!-- Desactivado -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</selector>

